  state: state.ground,
  currentAnimation: animations.idle,
}

class AnimatableObject {
  position: Vector3
  rotation: rotation
  model: Triangle[]

  constructor(position: Vector3, rotation: rotation, model: Triangle[]) {
    this.position = position
    this.rotation = rotation
    this.model = model
  }
}

// Calculates the normal vector for a triangle defined by three vertices.
function calculateNormal(v1: Vector3, v2: Vector3, v3: Vector3): Vector3 {
  const edge1 = v2.subtract(v1)
  const edge2 = v3.subtract(v1)
  const normal = edge1.cross(edge2)
  return normal.normalize()
}

// Scales a vector relative to a given center point.
function scaleVector3(vec: Vector3, center: Vector3, factor: number): Vector3 {
  const diff = vec.subtract(center)
  const scaledDiff = new Vector3(
    diff.x * factor,
    diff.y * factor,
    diff.z * factor,
  )
  return center.add(scaledDiff)
}

// Fills a horizontal line of pixels (scanline) between two x-coordinates
// at a given y-coordinate, performing Z-buffering and linear Z interpolation.
function fillScanline(
  y: number,
  x1: number, // Linke X Koordinate der Scanline (kann außerhalb des Bildschirms sein)
  x2: number, // Rechte X Koordinate der Scanline (kann außerhalb des Bildschirms sein)
  z1: number, // Z (Tiefe) Wert bei x1
  z2: number, // Z (Tiefe) Wert bei x2
  rByte: number,
  gByte: number,
  bByte: number,
  applyZBias: boolean,
) {
  const iy = Math.round(y)

  // Sicherstellen, dass die Scanline-Y-Koordinate im gültigen Bereich liegt.
  // Dies sollte zwar schon durch den Y-Loop in fillTriangleProjected geklemmt sein,
  // aber eine zusätzliche Prüfung schadet nicht.
  if (iy < 0 || iy >= screenDimensions.height) {
    return
  }

  // Runde die X-Koordinaten für die Pixel-Indizierung.
  // Diese können immer noch außerhalb des Bildschirms liegen.
  let ix1 = Math.round(x1)
  let ix2 = Math.round(x2)

  // Berechne die ursprüngliche Breite der Scanline für die Z-Interpolation.
  // Dies ist der Abstand in Pixeln zwischen ix1 und ix2 VOR dem Clipping.
  const originalWidth = ix2 - ix1

  // Klemme die X-Koordinaten auf die Bildschirmgrenzen [0, screenDimensions.width - 1].
  const screenWidth = screenDimensions.width
  const clippedIx1 = Math.max(0, Math.min(screenWidth - 1, ix1))
  const clippedIx2 = Math.max(0, Math.min(screenWidth - 1, ix2))

  // Wenn nach dem Klemmmen der Start-X-Wert größer ist als der End-X-Wert,
  // liegt die gesamte Scanline außerhalb des Bildschirms (horizontal).
  if (clippedIx1 > clippedIx2) {
    return
  }

  // Berechne die Z-Werte (Tiefen) an den Start- und Endpunkten der *geklippten* Scanline.
  // Wir müssen basierend auf dem Abstand entlang der *ursprünglichen* Scanline interpolieren.
  let clippedZ1 = z1
  let clippedZ2 = z2

  // Vermeide Division durch Null bei vertikalen Linien oder Punkten
  if (originalWidth !== 0) {
    // Anteil der ursprünglichen Breite bis zum Start des geklippten Segments.
    const t1 = (clippedIx1 - ix1) / originalWidth
    clippedZ1 = z1 + t1 * (z2 - z1) // Interpoliere Z

    // Anteil der ursprünglichen Breite bis zum Ende des geklippten Segments.
    const t2 = (clippedIx2 - ix1) / originalWidth
    clippedZ2 = z1 + t2 * (z2 - z1) // Interpoliere Z
  }

  // Wende einen kleinen Z-Bias an, um die Zeichenreihenfolge zu beeinflussen (z.B. für Outlines).
  // Der Bias wird auf die Z-Werte der geklippten Endpunkte angewendet.
  const biasedZ1 = clippedZ1 + (applyZBias ? 3 : 0)
  const biasedZ2 = clippedZ2 + (applyZBias ? 3 : 0)

  // Berechne den Z-Schritt (Delta Z pro Pixel) entlang der *geklippten* Scanline.
  const clippedWidth = clippedIx2 - clippedIx1
  let zStep = 0

  if (clippedWidth > 0) {
    // Vermeide Division durch Null für einzelne Pixel
    zStep = (biasedZ2 - biasedZ1) / clippedWidth
  }

  // Starte die Z-Interpolation am Z-Wert des geklippten Startpunkts.
  let currentZ = biasedZ1

  // Schleife über die Pixel des *geklippten* horizontalen Bereichs [clippedIx1, clippedIx2].
  for (let ix = clippedIx1; ix <= clippedIx2; ix++) {
    // setPixel führt die Bounds-Checks erneut durch (sicher ist sicher) und macht das Z-Buffering.
    setPixel(ix, iy, currentZ, rByte, gByte, bByte, 255)
    currentZ += zStep // Inkrementiere Z für das nächste Pixel auf der Scanline
  }
}

// Linearly interpolates between two 2D points based on a target y-coordinate,
// also interpolating the associated Z value. Used for finding points on triangle edges.
function interpolate(
  y: number,
  vA: { x: number; y: number; z: number },
  vB: { x: number; y: number; z: number },
): { x: number; z: number } | null {
  const yDiff = vB.y - vA.y

  if (Math.abs(yDiff) < epsilon) {
    if (Math.abs(y - vA.y) < epsilon) {
      return { x: vA.x, z: vA.z }
    }
    return null // Linie ist horizontal, aber die gesuchte Y-Koordinate liegt nicht darauf
  }

  const t = (y - vA.y) / yDiff
  if (t < -epsilon || t > 1 + epsilon) return null

  // MODIFIED: Diese Überprüfung wurde entfernt. Sie verhinderte, dass
  // Punkte außerhalb des Segments korrekt berechnet werden, was für Dreiecke
  // am Bildschirmrand notwendig ist. Das Clipping wird in fillScanline gehandhabt.
  // if (t < -epsilon || t > 1 + epsilon) return null

  const x = vA.x + t * (vB.x - vA.x)
  const z = vA.z + t * (vB.z - vA.z) // Interpoliere Z (Tiefe) genauso
  return { x, z } // Gibt den 2D Punkt mit Z zurück
}

// Renders a projected triangle to the screen using scanline filling and Z-buffering.
// Handles sorting vertices by Y and splitting complex triangles.
function fillTriangleProjected(triangle: Triangle) {
  // Input triangle ist in World Space

  // 1. Transformiere das Dreieck von Welt-Koordinaten in Kamera-Koordinaten (View Space)
  // Rufe die neue worldToView Funktion für jeden Eckpunkt auf.
  const v1View = worldToView(triangle.vertices[0])
  const v2View = worldToView(triangle.vertices[1])
  const v3View = worldToView(triangle.vertices[2])
  // Erstelle ein Dreieck-Array für die Clipping-Funktionen.
  const viewTriangle: ViewSpaceTriangle = [v1View, v2View, v3View]

  // 2. Back-face Culling (oft in View Space nach Transformation)
  // Berechne die Normale in View Space aus den View Space Eckpunkten.
  const v1v2View = v2View.subtract(v1View)
  const v1v3View = v3View.subtract(v1View)
  const normalView = v1v2View.cross(v1v3View).normalize()

  // Annahme: Kamera blickt in +Z Richtung in View Space.
  const cameraForwardViewActual = new Vector3(0, 0, 1)

  // Berechne das Skalarprodukt zwischen der Normalen im View Space und der Blickrichtung.
  const dotCullingCorrectedView = normalView.dot(cameraForwardViewActual)

  // Korrigiertes Back-face Culling: Blende aus, wenn die Normale vom Blickvektor weg zeigt.
  // Wenn die Normale von der Vorderseite des Dreiecks weg zeigt und normalView.dot(cameraForwardViewActual) > epsilon ist,
  // dann zeigt die VORDERSEITE vom Betrachter weg. Verwende epsilon für einen robusten Float-Vergleich.
  // Blende aus, wenn dotProduct > epsilon und es KEIN Spieler-Dreieck ist.
  if (dotCullingCorrectedView > epsilon && !triangle.isPlayer) {
    // console.log("Culling backface (korrigiert)"); // Debugging
    return // Blende diese Rückseite aus
  }

  // 3. Clipping gegen Nah-Ebene (und dann ggf. weitere Ebenen)
  // DIESER SCHRITT ERSETZT den alten Check 'if (projectedVertices.some((v) => v === null))'.
  // Rufe die Clipping-Funktion für die Nah-Ebene mit dem View Space Dreieck auf.
  // Die Funktion gibt eine Liste von 0, 1 oder 2 Dreiecken in View Space zurück.
  let clippedTrianglesView = clipTriangleAgainstNearPlane(viewTriangle)

  // TODO: Wenn du Clipping gegen weitere Ebenen (Fern, Links, Rechts, Oben, Unten) implementierst,
  // würde hier eine Kaskade von Clipping-Aufrufen folgen. Jede Stufe nimmt die Dreiecke der vorherigen.
  // Z.B.:
  // let finalClippedTriangles: ViewSpaceTriangle[] = [];
  // clippedTrianglesView.forEach(tri => {
  //    const clippedAgainstFar = clipTriangleAgainstFar(tri); // clipTriangleAgainstFar muss implementiert werden
  //    clippedAgainstFar.forEach(farTri => {
  //        const clippedAgainstLeft = clipTriangleAgainstLeft(farTri); // clipTriangleAgainstLeft muss implementiert werden
  //        clippedAgainstLeft.forEach(leftTri => {
  //            // ... usw. bis alle 6 Ebenen durch sind ...
  //            finalClippedTriangles.push(...ergebnis);
  //        });
  //    });
  // });

  // Für diese Implementierung (nur Near-Clipping) sind die Ergebnisse von clipTriangleAgainstNearPlane
  // die finalen geklippten Dreiecke, die weiterverarbeitet werden.
  let finalClippedTriangles = clippedTrianglesView // Die Liste der Dreiecke nach Near-Clipping

  // 4. Projektion auf 2D Bildschirm-Koordinaten und Rasterisierung
  // Gehe durch JEDES der resultierenden geklippten Dreiecke (0, 1 oder 2 Stück).
  finalClippedTriangles.forEach((clippedTriView) => {
    // Iteriere über die geklippten Dreiecke (in View Space)

    // Für jedes geklippte Dreieck: Projiziere seine 3 Eckpunkte von View Space nach Screen Space
    // Verwende die viewToScreen Funktion, die KEINEN Z<=0.1 Check macht, da Punkte geklippt sind.
    const p1 = viewToScreen(clippedTriView[0]) // Aufruf der viewToScreen Funktion
    const p2 = viewToScreen(clippedTriView[1])
    const p3 = viewToScreen(clippedTriView[2])

    // Nun folgt deine bisherige Rasterisierungslogik, die mit diesen 2D-projizierten Punkten arbeitet.
    // Diese Punkte p1, p2, p3 sind die {x, y, z} Objekte, die du bisher von project bekommen hast.

    // Sortiere die 3 projizierten Eckpunkte nach ihrer 2D y-Koordinate für die Scanline-Füllung
    const projectedPoints2D = [p1, p2, p3] // Dies sind {x, y, z} Objekte nach Projektion
    projectedPoints2D.sort((a, b) => a.y - b.y) // Sortiere nach der 2D y-Koordinate

    const pp1 = projectedPoints2D[0] // Der oberste Punkt
    const pp2 = projectedPoints2D[1] // Der mittlere Punkt
    const pp3 = projectedPoints2D[2] // Der unterste Punkt

    // Bestimme die Farbe und Beleuchtung für dieses Dreieck
    let rFloat = 1.0,
      gFloat = 1.0,
      bFloat = 1.0
    const initialColor = hexToRgb(triangle.color) // Nutze die Farbe des Originaldreiecks

    if (initialColor !== null) {
      rFloat = initialColor.r
      gFloat = initialColor.g
      bFloat = initialColor.b

      if (!triangle.bakedLight) {
        // Beleuchtung: Nutze die Normale des Originaldreiecks für diffuse Beleuchtung.
        // Eine präzisere Implementierung würde die Normale des geklippten Dreiecks verwenden,
        // falls sich die Orientierung signifikant ändert. Hier verwenden wir die Originalnormale.
        const lightLevels = calculateLightLevel(triangle.normal)
        rFloat = Math.max(0, Math.min(1, rFloat + lightLevels))
        gFloat = Math.max(0, Math.min(1, gFloat + lightLevels))
        bFloat = Math.max(0, Math.min(1, bFloat + lightLevels))
      }
    }

    const rByte = Math.floor(rFloat * 255)
    const gByte = Math.floor(gFloat * 255)
    const bByte = Math.floor(bFloat * 255)

    // Rasterisiere das Dreieck, indem horizontale Scanlines gefüllt werden.
    // Gehe durch die Y-Koordinaten (Pixelreihen) von der oberen Spitze zur unteren Spitze.
    const startY = Math.round(pp1.y)
    const endY = Math.round(pp3.y)

    // Klemme den Y-Loop-Bereich an die Bildschirmhöhe, um Robustheit zu gewährleisten.
    const clampedStartY = Math.max(
      0,
      Math.min(screenDimensions.height - 1, startY),
    )
    const clampedEndY = Math.max(0, Math.min(screenDimensions.height - 1, endY))

    for (let y = clampedStartY; y <= clampedEndY; y++) {
      // Finde die Schnittpunkte dieser horizontalen Scanline (bei Y) mit den Kanten des geklippten, projizierten Dreiecks.
      // Nutze die interpolate Funktion (die für Scanlines), die x und z interpoliert.
      // Die Kanten sind V1-V3 (immer) und entweder V1-V2 (obere Hälfte) oder V2-V3 (untere Hälfte).
      const scanlineP1 = interpolate(y, pp1, pp3) // Schnittpunkt mit der Kante von oberster zu unterster Spitze

      let scanlineP2 = null
      if (y < pp2.y) {
        // Wenn die Scanline in der oberen Hälfte des Dreiecks ist, schneidet sie die Kante V1-V2.
        scanlineP2 = interpolate(y, pp1, pp2)
      } else {
        // Wenn die Scanline in der unteren Hälfte ist (oder genau auf pp2.y), schneidet sie die Kante V2-V3.
        scanlineP2 = interpolate(y, pp2, pp3)
      }

      // Wenn gültige Schnittpunkte für diese Scanline gefunden wurden (sollte meist der Fall sein im geklemmten Bereich):
      if (scanlineP1 !== null && scanlineP2 !== null) {
        // Sicherstellen, dass scanlineP1 der linkere und scanlineP2 der rechtere Punkt ist.
        // fillScanline erwartet dies.
        if (scanlineP1.x < scanlineP2.x) {
          fillScanline(
            y,
            scanlineP1.x,
            scanlineP2.x,
            scanlineP1.z,
            scanlineP2.z,
            rByte,
            gByte,
            bByte,
            triangle.applyZBias,
          )
        } else {
          fillScanline(
            y,
            scanlineP2.x,
            scanlineP1.x,
            scanlineP2.z,
            scanlineP1.z,
            rByte,
            gByte,
            bByte,
            triangle.applyZBias,
          )
        }
      }
    }
  }) // Ende der Schleife über die resultierenden geklippten Dreiecke
}

// Represents a renderable triangle with color, normal, and lighting properties.
class Triangle {
  vertices: [Vector3, Vector3, Vector3]
  color: string
  normal: Vector3
  bakedLight: boolean
  isPlayer: boolean
  applyZBias: boolean // For Z-buffering order control

  constructor(
    v1: Vector3,
    v2: Vector3,
    v3: Vector3,
    color: string = "white",
    normal: Vector3,
    bakedLight: boolean,
    isPlayer: boolean = false,
    applyZBias: boolean = false,
  ) {
    this.vertices = [v1, v2, v3]
    this.color = color
    this.normal = normal
    this.bakedLight = bakedLight
    this.isPlayer = isPlayer
    this.applyZBias = applyZBias
  }
}

// Represents a triangle used for collision detection.
class collisionTriangle {
  vertices: [Vector3, Vector3, Vector3]
  normal: Vector3
  collisionType: collisionTypes
  originOffset: number // Distance from origin to the plane along the normal

  constructor(
    v1: Vector3,
    v2: Vector3,
    v3: Vector3,
    normal: Vector3,
    collisionType: collisionTypes = collisionTypes.wall,
  ) {
    this.originOffset = 0
    this.vertices = [v1, v2, v3]
    this.collisionType = collisionType
    this.normal = normal
  }

  // Updates collision data based on potentially transformed world vertices and normal.
  updateCollisionData(
    worldVertices: [Vector3, Vector3, Vector3],
    worldNormal: Vector3,
  ): void {
    this.vertices = worldVertices
    this.normal = worldNormal.normalize()

    // Determine collision type based on the up component of the normal
    if (this.normal.y > 0.01) {
      this.collisionType = collisionTypes.floor
    } else if (this.normal.y < -0.01) {
      this.collisionType = collisionTypes.ceiling
    } else {
      this.collisionType = collisionTypes.wall
    }
    // Calculate the plane equation constant (Ax + By + Cz + D = 0, where D = -normal.dot(vertex))
    this.originOffset = -this.normal.dot(this.vertices[0])
  }
}

const LEVEL_BOUNDARY_MAX = 200 // Max extent of the level in world units
const CELL_SIZE = 40 // Size of a grid cell in world units
const LEVEL_DIMENSION = LEVEL_BOUNDARY_MAX * 2 // Total dimension of the level (e.g., 400)
const NUM_CELLS_PER_DIMENSION = Math.ceil(LEVEL_DIMENSION / CELL_SIZE) // Number of grid cells along one dimension
const NUM_CELLS_INDEX = NUM_CELLS_PER_DIMENSION // Alias for clarity

let staticSurfacePartitionTriangles: collisionTriangle[] = []
const staticSurfacePartition: number[][][] = []

const tv1 = new Vector3(10, 7, 10)
const tv2 = new Vector3(-10, 0, -20) // Der Wert, der vorher tv3 zugewiesen war
const tv3 = new Vector3(-10, 0, 10)

const tv11 = new Vector3(30, 0, 30)
const tv21 = new Vector3(-30, 0, -30) // Der Wert, der vorher tv3 zugewiesen war
const tv31 = new Vector3(-30, 0, 30)

const tv12 = new Vector3(-30, 0, -30)
const tv22 = new Vector3(30, 0, 30) // Der Wert, der vorher tv3 zugewiesen war
const tv32 = new Vector3(30, 0, -30)

let scene: Triangle[] = [
  new Triangle(tv1, tv2, tv3, "#00FFFF", calculateNormal(tv1, tv2, tv3), false),
  new Triangle(
    tv11,
    tv21,
    tv31,
    "#009999",
    calculateNormal(tv11, tv21, tv31),
    false,
  ),
  new Triangle(
    tv12,
    tv22,
    tv32,
    "#006666",
    calculateNormal(tv12, tv22, tv32),
    false,
  ),
]

setupStaticCollisionGeometry()
console.log(staticSurfacePartition)
console.log(staticSurfacePartitionTriangles)

function initializeSpatialPartition() {
  for (let z = 0; z < NUM_CELLS_PER_DIMENSION; z++) {
    staticSurfacePartition[z] = new Array<number[]>(NUM_CELLS_PER_DIMENSION)

    for (let x = 0; x < NUM_CELLS_PER_DIMENSION; x++) {
      staticSurfacePartition[z][x] = [] as number[] // Ensure each cell is an array
    }
  }
  console.log(
    `Räumliche Partition initialisiert: ${NUM_CELLS_PER_DIMENSION}x${NUM_CELLS_PER_DIMENSION} Zellen.`,
  )
}

// Sets up the static collision geometry by creating collisionTriangle objects
// and distributing them into the spatial partition grid.
function setupStaticCollisionGeometry() {
  initializeSpatialPartition() // Make sure the grid is initialized

  const rawCollisionTrianglesData = [
    {
      vertices: [tv1, tv2, tv3],
      normal: calculateNormal(tv1, tv2, tv3),
    },
    {
      vertices: [tv11, tv21, tv31],
      normal: calculateNormal(tv11, tv21, tv31),
    },
    {
      vertices: [tv12, tv22, tv32],
      normal: calculateNormal(tv12, tv22, tv32),
    },
    // Add more raw collision data here as needed
  ]

  rawCollisionTrianglesData.forEach((data) => {
    const colTriangle = new collisionTriangle(
      data.vertices[0],
      data.vertices[1],
      data.vertices[2],
      data.normal,
    )

    const worldVertices = data.vertices
    const worldNormal = data.normal

    // Update collision data with world coordinates (important if geometry is transformed later)
    colTriangle.updateCollisionData(
      worldVertices as [Vector3, Vector3, Vector3],
      worldNormal,
    )
    staticSurfacePartitionTriangles.push(colTriangle)
    addTriangleToPartition_BoundingBox(colTriangle)
  })

  console.log("Statische Kollisionsgeometrie im Grid eingerichtet.")
}

// Converts world coordinates (x, z) into corresponding cell indices in the spatial partition grid.
function getWorldCoordsCellIndices(
  worldX: number,
  worldZ: number,
): { cellX: number; cellZ: number } {
  // Shift world coordinates so that (-LEVEL_BOUNDARY_MAX, -LEVEL_BOUNDARY_MAX) maps to (0, 0) in the grid
  const cellX = Math.floor((worldX + LEVEL_BOUNDARY_MAX) / CELL_SIZE)
  const cellZ = Math.floor((worldZ + LEVEL_BOUNDARY_MAX) / CELL_SIZE)

  return { cellX: cellX, cellZ: cellZ }
}

// Adds a collision triangle to all grid cells that its bounding box intersects.
// This allows for efficient querying of nearby collision triangles.
function addTriangleToPartition_BoundingBox(triangle: collisionTriangle): void {
  // Calculate the bounding box of the triangle in world space
  const minX = Math.min(
    triangle.vertices[0].x,
    triangle.vertices[1].x,
    triangle.vertices[2].x,
  )
  const maxX = Math.max(
    triangle.vertices[0].x,
    triangle.vertices[1].x,
    triangle.vertices[2].x,
  )
  const minZ = Math.min(
    triangle.vertices[0].z,
    triangle.vertices[1].z,
    triangle.vertices[2].z,
  )
  const maxZ = Math.max(
    triangle.vertices[0].z,
    triangle.vertices[1].z,
    triangle.vertices[2].z,
  )

  // Get the grid cell indices for the corners of the bounding box
  const startCellIndices = getWorldCoordsCellIndices(minX, minZ)
  const startCellX = startCellIndices.cellX
  const startCellZ = startCellIndices.cellZ

  const endCellIndices = getWorldCoordsCellIndices(maxX, maxZ)
  const endCellX = endCellIndices.cellX
  const endCellZ = endCellIndices.cellZ

  // Clamp indices to be within the grid boundaries
  const clampedStartCellX = Math.max(0, startCellX)
  const clampedEndCellX = Math.min(NUM_CELLS_PER_DIMENSION - 1, endCellX)
  const clampedStartCellZ = Math.max(0, startCellZ)
  const clampedEndCellZ = Math.min(NUM_CELLS_PER_DIMENSION - 1, endCellZ)

  // Iterate over all cells intersected by the bounding box and add the triangle to their list
  for (let z = clampedStartCellZ; z <= clampedEndCellZ; z++) {
    for (let x = clampedStartCellX; x <= clampedEndCellX; x++) {
      // Basic check to ensure the cell is an array before pushing
      if (Array.isArray(staticSurfacePartition[z]?.[x])) {
        console.log(
          `DEBUG: Vor push - Zelle [${z}][${x}], Länge: ${staticSurfacePartition[z][x].length}`,
        )
        console.log(`DEBUG: Objekt, das gepusht werden soll: `, triangle)

        staticSurfacePartition[z][x].push(
          staticSurfacePartitionTriangles.length - 1,
        )

        console.log(
          `DEBUG: Nach push - Zelle [${z}][${x}], Länge: ${staticSurfacePartition[z][x].length}`,
        )
      } else {
        // Error handling if a cell is not initialized correctly
        console.error(
          `FEHLER: staticSurfacePartition[${z}][${x}] ist KEIN Array! Typ: ${typeof staticSurfacePartition[z]?.[x]}`,
          staticSurfacePartition[z]?.[x],
        )
      }
    }
  }
}

// Calculates a 3D vector representing the direction of light
// based on its yaw and pitch angles.
function calculateLightDirection(yaw: number, pitch: number): Vector3 {
  // Start with a vector pointing along the positive Z axis (or chosen forward direction)
  let dir = new Vector3(0, 0, 1)

  // Apply pitch rotation around the X axis
  const cosPitch = Math.cos(pitch)
  const sinPitch = Math.sin(pitch)
  const pitchedY = dir.y * cosPitch - dir.z * sinPitch
  const pitchedZ = dir.y * sinPitch + dir.z * cosPitch
  dir.y = pitchedY
  dir.z = pitchedZ

  // Apply yaw rotation around the Y axis
  const cosYaw = Math.cos(yaw)
  const sinYaw = Math.sin(yaw)
  const yawedX = dir.x * cosYaw + dir.z * sinYaw
  const yawedZ = dir.z * cosYaw - dir.x * sinYaw
  dir.x = yawedX
  dir.z = yawedZ

  return dir.normalize() // Return a unit vector
}

// Calculates a light intensity factor for a surface based on its normal
// and the global light direction using the dot product.
function calculateLightLevel(normal: Vector3): number {
  // Dot product gives a value between -1 (surface facing away from light)
  // and 1 (surface facing directly at light).
  const dot = normal.dot(lightDir)

  // Shift and scale the dot product to get a light level (e.g., 0 to 1 or adjusted range).
  // Adding 0.5 shifts the range from [-1, 1] to [-0.5, 1.5].
  const levelFloat = dot + 0.5

  // Dividing by 8 further scales the light level (adjust as needed for desired brightness).
  return levelFloat / 8
}

function getRandomColor() {
  const letters = "0123456789ABCDEF"
  let color = "#"
  for (let i = 0; i < 6; i++) {
    color += letters[Math.floor(Math.random(0, 1) * 16)]
  }
  return color
}

